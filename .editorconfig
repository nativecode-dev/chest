[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true

[*.cs]
indent_size = 4

[*.html]
indent_size = 2

[*.js]
indent_size = 2

[*.json]
indent_size = 2

[*.ts]
indent_size = 2

[*.tsx]
indent_size = 2

[*.yml
indent_size = 2

[*.md]
trim_trailing_whitespace = false
